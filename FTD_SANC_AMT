<<<<<<< HEAD
SANCTION

FTD_SANC_AMT

CALCULATE(
    SUM('Business'[Amount]),
    FILTER(
        'Business',
        'Business'[Date] = 'Business'[FTD] &&
        'Business'[Status] = "SANCTION"
        )
)

FTD_SANC_NUM

=CALCULATE(
    SUM('Business'[No]),
    FILTER(
        'Business',
        'Business'[Date]='Business'[FTD] &&
        'Business'[Status] = "SANCTION"
        )
)

MTD_SANC_AMT

=CALCULATE(
    SUM('Business'[Amount]),
    FILTER(
        'Business',
        YEAR('Business'[Date]) = YEAR(Business[FTD]) &&
        MONTH('Business'[Date]) = MONTH(Business[FTD]) &&
        'Business'[Date] <= Business[FTD] &&
        'Business'[Status] = "SANCTION"
    )
)

MTD_SANC_NUM

=CALCULATE(
    SUM('Business'[No]),
    FILTER(
        'Business',
        YEAR('Business'[Date]) = YEAR(Business[FTD]) &&
        MONTH('Business'[Date]) = MONTH(Business[FTD]) &&
        'Business'[Date] <= Business[FTD] &&
        'Business'[Status] = "SANCTION"
    )
)



